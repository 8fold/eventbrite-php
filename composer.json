{
    "name": "8fold/php-eventbrite-sdk",
    "description": "Something that seems easier and more complete than the official Eventbrite SDK for PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Bruce",
            "email": "josh@joshuabruce.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Eightfold\\Eventbrite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eightfold\\Eventbrite\\Tests\\": "tests/"    
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
